#include <stdio.h>
int main(void)
{
 int x, y;
 printf("2개의 정수를 입력하시오:");

 scanf("%d%d", &x, &y);

 if (x == 0)
 {
  if (y == 0)
   printf("두수 모두 0입니다.\n");
  else if (y == 1)
   printf("x는 0이고 y는 0이 아닙니다.\n");
 }
 
 else
 {
  if (y == 0)
   printf("x는 0이 아니고 y는 0입니다..\n");
  else if (y == 1)
   printf("두 수 모두 1입니다.\n");
 }


 return 0;
}
